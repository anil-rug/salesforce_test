{
  "$schema": "https://di.schema.mediact.com/register.configuration.json",
  "type": "storage",
  "identifier": "salesforce-token-cache",
  "name": "Salesforce Oauth 2.0 token cache",
  "object": {
    "prototype": "file",
    "parameters": {
      "ttl": {
        "parameters": "10 minutes"
      },
      "path": "salesforce-token-cache",
      "filesystem": "default-storage-filesystem"
    }
  }
}
